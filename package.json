{
  "name": "next-starter-app-jr",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
